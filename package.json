{
  "name": "ak-chars",
  "version": "0.1.0",
  "type": "module",
  "description": "Small TypeScript scaffold",
  "scripts": {
    "test": "node --experimental-vm-modules --loader ts-node/esm node_modules/.bin/jest --config ./jest.config.ts --verbose",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "preinstall": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "pretest": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "prestart": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "prebuild": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "prepack": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "prepublishOnly": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "preversion": "if [ \"$CI\" != \"1\" ]; then tsx ./scripts/blockNpm.ts; fi",
    "format": "prettier --write 'src/**/*.{ts,json,md}'",
    "extract-tiers": "tsx ./scripts/extractTiers.ts",
    "fetch-professions": "tsx ./scripts/fetchProfessions.ts",
    "fetch-chars": "tsx ./scripts/fetchChars.ts",
    "fetch-avatars": "tsx ./scripts/fetchAvatars.ts",
    "optimize-avatars": "tsx ./scripts/optimizeAvatars.ts",
    "dev": "vite --config vite.config.ts",
    "start-bg": "bash ./scripts/start-dev.sh",
    "stop-bg": "bash ./scripts/stop-dev.sh",
    "build": "vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "storybook": "sb dev -p 6006 --no-open"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/cli": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/jest": "^29.5.0",
    "@types/jpeg-js": "^0.3.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.5.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/upng-js": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^8.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^14.0.0",
    "prettier": "^2.8.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,css,md}": [
      "yarn lint",
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "image-q": "^4.0.0",
    "jpeg-js": "^0.4.0",
    "pngjs": "^6.0.0",
    "upng-js": "^2.1.0"
  }
}
