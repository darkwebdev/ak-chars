{
  "name": "ak-chars",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "Small TypeScript scaffold",
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.{ts,json,md}'",
    "extract-tiers": "ts-node ./scripts/extractTiers.ts",
    "fetch-chars": "ts-node ./scripts/fetchChars.ts",
    "fetch-avatars": "ts-node ./scripts/fetchAvatars.ts",
    "optimize-avatars": "ts-node ./scripts/optimizeAvatars.ts",
    "dev": "vite --config vite.config.ts",
    "start-bg": "bash ./scripts/start-dev.sh",
    "stop-bg": "bash ./scripts/stop-dev.sh",
    "build": "vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "storybook": "sb dev -p 6006 --no-open"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/cli": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/jest": "^29.5.0",
    "@types/jpeg-js": "^0.3.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.5.0",
    "@types/pngjs": "^6.0.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.6.0",
    "prettier": "^2.8.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0"
  },
  "dependencies": {
    "jpeg-js": "^0.4.0",
    "pngjs": "^6.0.0"
  }
}
